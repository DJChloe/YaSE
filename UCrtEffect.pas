{ TCrtFilter }

constructor TCrtFilter.Create;
begin
  inherited;
  FShaders[0] := CreateContextShader;
end;

destructor TCrtFilter.Destroy;
begin
  inherited;
end;

procedure TCrtFilter.LoadShaders;
var
  AWidth, AHeight: Single;
begin
  AWidth := InputSize.Width;
  AHeight := InputSize.Height;

  TFilterManager.FilterContext.SetShaders(FVertexShader, FShaders[FPass]);
  TFilterManager.FilterContext.SetShaderVariable('p0',
    [Vector3D(AWidth, AHeight,
     TThread.GetTickCount / 1000, ValuesAsFloat['Distortion'])]);

  TFilterManager.FilterContext.SetShaderVariable('p1',
    [Vector3D(ValuesAsFloat['Flicker'], ValuesAsFloat['Pixelshift'],
    ValuesAsFloat['Vignette'], ValuesAsFloat['Scanlines'])]);
end;

class function TCrtFilter.FilterAttr: TFilterRec;
begin
  result := TFilterRec.Create('CrtFilter',
    'An effect that simulates a CRT Monitor.',
    [TFilterValueRec.Create('Distortion', 'CRT Monitor Radial Distorion', TFilterValueType.Float, 1, 0, 1),
     TFilterValueRec.Create('Flicker', 'Screen Flickering', TFilterValueType.Float, 1, 0, 1),
    TFilterValueRec.Create('Pixelshift', 'Pixelshift Method (0=none,1=hylian,2=sinus)', TFilterValueType.Float, 2, 0, 2),
    TFilterValueRec.Create('Scanlines', 'Use Scanlines', TFilterValueType.Float, 1, 0, 0)]);
end;

{ TCrtEffect }

constructor TCrtEffect.Create(AOwner: TComponent);
begin
  inherited;
  FEffectStyle := [TEffectStyle.DisablePaint];
end;

function TCrtEffect.CreateFilter: TFilter;
begin
  result := TCrtFilter.Create;
end;

function TCrtEffect.GetOffset: TPointF;
begin
  result := TPointF.Create(0, 0);
end;

function TCrtEffect.GetRect(const ARect: TRectF): TRectF;
begin
  result := ARect;
  result.Inflate(0, 0);
end;

{ TYuvTextureMaterialSource }

function TCrtTextureMaterialSource.CreateMaterial: TMaterial;
begin
  result := TCrtTextureMaterial.Create;
end;

{ TYuvTextureMaterial }

procedure TCrtTextureMaterial.DoApply(const Context: TContext3D);
begin
  if not assigned(Texture) then
    exit;
  FCounter := FCounter + 0.04;
  Context.SetShaders(FVertexShader, FPixelShader);
  Context.SetShaderVariable('texture0', Texture);
  Context.SetShaderVariable('p0',
    [Vector3D(FWidth, FHeight, Fcounter,
    Integer(FDistortion))]);

  Context.SetShaderVariable('p1',
    [Vector3D(integer(FFlicker), single(FPixelshift), Integer(FVignette),
    integer(FScanlines))]);
end;

procedure TCrtTextureMaterial.DoInitialize;
begin
  inherited;
  FPixelShader := CreateContextShader('texture0');
  FDistortion := true;
  FFlicker := true;
  FPixelshift := 2;
  FVignette := true;
  FScanlines := true;
end;

end.
